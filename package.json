{
  "name": "@cloudsdk/test-runner",
  "description": "Cloud SDK Test Runner",
  "version": "1.5.2",
  "author": "Florin Mihalache",
  "bin": {
    "csdk-test-runner": "./bin/run"
  },
  "bugs": "https://bitbucket.1and1.org/csdk/test-runner",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^2.2.3",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.0",
    "execa": "^4.0.3",
    "jsonpath-plus": "^4.0.0",
    "listr": "^0.14.3",
    "tslib": "^1.13.0",
    "typescript-json-schema": "^0.42.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.12",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.28",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://bitbucket.1and1.org/csdk/test-runner/test-runner",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "csdk-test-runner",
    "dirname": "csdk-test-runner",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://bitbucket.1and1.org/csdk/test-runner",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
